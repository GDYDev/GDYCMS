<!------------------------------------------------------------------CLIPBOARD----------------------------------------------------->
<div id="LinkBufferModal" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">LINK BUFFR</h4>
      </div>
      <div class="modal-body">
        <div class="row">        
            <div class="col-md-12 col-xs-12">
                <select multiple="multiple" style="width:100%;height:100%;" ng-model="LinkBuffer.SelectedLink">
                    <option ng-repeat="item in LinkBuffer.ObjectList" value="{{item.Address}}">{{item.LinkName}}</option>
                </select>
            </div>        
        </div>
        <div class="row">
            <div class="col-md-12 col-xs-12">{{LinkBufferModel.SelectedLink}}</div>
        </div>
      </div>
        <button type="button" class="btn btn-danger LeftModalButton" ng-click="$('#LinkBufferModal').modal('lock');LinkBuffer.DeleteSelected();">REMOVE</button>
        <button type="button" class="btn btn-default RightModalButton" data-dismiss="modal">CLOSE</button>
      <div class="modal-footer">

      </div>
    </div>
  </div>
</div>

