<div id="FileEmpty" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Error</h4>
      </div>
      <div class="modal-body">
        Sorry, but file field is empty
      </div>                
      <div class="modal-footer">
          <button type="button" class="btn btn-primary RightModalButton" data-dismiss="modal">CLOSE</button>
      </div>
    </div>
  </div>
</div>

<div id="RenameOperation" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Rename</h4>
      </div>
      <div class="modal-body">
        <pre>
        Old name of element is {{FileManager.Instance.SelectedObject.Name}}
        New name of element is <input type="text" ng-model="FileManager.Instance.NewObjectName"/>
        </pre>
      </div>                
      <div class="modal-footer">
          <button type="button" class="btn btn-primary RightModalButton" data-dismiss="modal">CANCEL</button>
          <button class="btn btn-success LeftModalButton" data-dismiss="modal" ng-click="FileManager.TryToRename();">RENAME</button>
      </div>
    </div>
  </div>
</div>

<!---------------------------------------------------------------------RENAME ERROR-------------------------------------------------->
<div id="ElementExists" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Error</h4>
      </div>
      <div class="modal-body">
